{
  "name": "SuperHero",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "alias": {
      "type": "string"
    },
    "ProtectionArea": {
      "type": "ProtectionArea"
    }
  },
  "validations": [],
  "relations": {
    "protectionAreas": {
      "type": "hasOne",
      "model": "ProtectionArea",
      "foreignKey": "superHeroId",
      "options": {
        "nestRemoting": true
      }
    },
    "superPowers": {
      "type": "hasMany",
      "model": "SuperPower",
      "foreignKey": "superHeroId",
      "through": "SuperHeroSuperPower"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
